{
  "check": {
    "toolVersion": "1.0.0",
    "variables": {
      "WORKLOAD_SDK_BAND_PATTERN": "6.0.*",
      "WORKLOAD_PREVIEW": "",
      "OPENJDK_VERSION": "11.0.12",
      "PACKAGE_SOURCE_NUGET_ORG": "https://api.nuget.org/v3/index.json"
    },
    "variableMappers": [
      {
        "$type": "DotNetCheck.Manifest.NuGetFeedWorkloadManifestSdkBandVariableMapper, MauiCheck",
        "packageSources": [ "$(PACKAGE_SOURCE_NUGET_ORG)" ],
        "mappings": [
          {
            "packageId": "Microsoft.NET.Sdk.Maui.Manifest-",
            "sdkBand": "$(WORKLOAD_SDK_BAND_PATTERN)",
            "packageVersion": "*$(WORKLOAD_PREVIEW)",
            "includePrerelease": false,
            "sdkBandVariableName": "MAUI_WORKLOAD_SDK_BAND",
            "packageIdVariableName": "MAUI_WORKLOAD_MANIFEST_PACKAGE_ID",
            "packageVersionVariableName": "MAUI_WORKLOAD_MANIFEST_PACKAGE_VERSION"
          },
          {
            "packageId": "Microsoft.NET.Sdk.Android.Manifest-",
            "sdkBand": "$(WORKLOAD_SDK_BAND_PATTERN)",
            "packageVersion": "*$(WORKLOAD_PREVIEW)",
            "includePrerelease": false,
            "sdkBandVariableName": "ANDROID_WORKLOAD_SDK_BAND",
            "packageIdVariableName": "ANDROID_WORKLOAD_MANIFEST_PACKAGE_ID",
            "packageVersionVariableName": "ANDROID_WORKLOAD_MANIFEST_PACKAGE_VERSION"
          },
          {
            "packageId": "Microsoft.NET.Sdk.MacCatalyst.Manifest-",
            "sdkBand": "$(WORKLOAD_SDK_BAND_PATTERN)",
            "packageVersion": "*$(WORKLOAD_PREVIEW)",
            "includePrerelease": false,
            "sdkBandVariableName": "MACCATALYST_WORKLOAD_SDK_BAND",
            "packageIdVariableName": "MACCATALYST_WORKLOAD_MANIFEST_PACKAGE_ID",
            "packageVersionVariableName": "MACCATALYST_WORKLOAD_MANIFEST_PACKAGE_VERSION"
          },
          {
            "packageId": "Microsoft.NET.Sdk.tvOS.Manifest-",
            "sdkBand": "$(WORKLOAD_SDK_BAND_PATTERN)",
            "packageVersion": "*$(WORKLOAD_PREVIEW)",
            "includePrerelease": false,
            "sdkBandVariableName": "TVOS_WORKLOAD_SDK_BAND",
            "packageIdVariableName": "TVOS_WORKLOAD_MANIFEST_PACKAGE_ID",
            "packageVersionVariableName": "TVOS_WORKLOAD_MANIFEST_PACKAGE_VERSION"
          },
          {
            "packageId": "Microsoft.NET.Sdk.macOS.Manifest-",
            "sdkBand": "$(WORKLOAD_SDK_BAND_PATTERN)",
            "packageVersion": "*$(WORKLOAD_PREVIEW)",
            "includePrerelease": false,
            "sdkBandVariableName": "MACOS_WORKLOAD_SDK_BAND",
            "packageIdVariableName": "MACOS_WORKLOAD_MANIFEST_PACKAGE_ID",
            "packageVersionVariableName": "MACOS_WORKLOAD_MANIFEST_PACKAGE_VERSION"
          },
          {
            "packageId": "Microsoft.NET.Sdk.iOS.Manifest-",
            "sdkBand": "$(WORKLOAD_SDK_BAND_PATTERN)",
            "packageVersion": "*$(WORKLOAD_PREVIEW)",
            "includePrerelease": false,
            "sdkBandVariableName": "IOS_WORKLOAD_SDK_BAND",
            "packageIdVariableName": "IOS_WORKLOAD_MANIFEST_PACKAGE_ID",
            "packageVersionVariableName": "IOS_WORKLOAD_MANIFEST_PACKAGE_VERSION"
          }
        ]
      },
      {
        "$type": "DotNetCheck.Manifest.DotNetSdkReleasesVariableMapper, MauiCheck",
        "channelVersion": "net6.0",
        "name":  "DOTNET_SDK_VERSION"
      }
    ],
    "openjdk": {
      "version": "11.0",
      "urls": {
        "win64": "https://aka.ms/download-jdk/microsoft-jdk-$(OPENJDK_VERSION)-windows-x64.msi",
        "osx": "https://aka.ms/download-jdk/microsoft-jdk-$(OPENJDK_VERSION)-macos-x64.pkg"
      }
    },
    "xcode": {
      "exactVersion": "21335",
      "exactVersionName": "14.0"
    },
    "vswin": {
      "minimumVersion": "17.3.6"
    },
    "vsmac": {
      "minimumVersion": "17.3.8.5",
      "optional": "true"
    },
    "android": {
      "packages": [
        {
          "path": "emulator",
          "version": "31.2.9"
        },
        {
          "path": "build-tools;31.0.0",
          "version": "31.0.0"
        },
        {
          "path": "platforms;android-31",
          "version": "1"
        },
        {
          "path": "system-images;android-31;google_apis;x86_64",
          "version": "3",
          "alternatives": [
            {
              "path": "system-images;android-31;google_apis_playstore;x86_64",
              "version": "9"
            },
            {
              "path": "system-images;android-31;google_apis;arm64-v8a",
              "version": "9"
            }
          ]
        },
        {
          "path": "platform-tools",
          "version": "33.0.2"
        },
        {
          "path": "cmdline-tools;7.0",
          "version": "7.0"
        }
      ],
      "emulators": [
        {
          "sdkId": "system-images;android-31;google_apis;x86_64",
          "alternateSdkIds": [
            "system-images;android-31;google_apis_playstore;x86_64",
            "system-images;android-31;google_apis;arm64-v8a"
          ],
          "desc": "Android Emulator - x86_64 - API31 - Google API's",
          "apiLevel": 31,
          "tag": "google_apis",
          "device": "pixel"
        }
      ]
    },
    "dotnet": {
      "sdks": [
        {
          "version": "$(DOTNET_SDK_VERSION)",
          "requireExact": false,
          "enableWorkloadResolver": false,
          "urls": {
            "win64": "https://dotnetcli.azureedge.net/dotnet/Sdk/$(DOTNET_SDK_VERSION)/dotnet-sdk-$(DOTNET_SDK_VERSION)-win-x64.exe",
            "win": "https://dotnetcli.azureedge.net/dotnet/Sdk/$(DOTNET_SDK_VERSION)/dotnet-sdk-$(DOTNET_SDK_VERSION)-win-x86.exe",
            "osx": "https://dotnetcli.azureedge.net/dotnet/Sdk/$(DOTNET_SDK_VERSION)/dotnet-sdk-$(DOTNET_SDK_VERSION)-osx-x64.pkg",
            "osxArm64": "https://dotnetcli.azureedge.net/dotnet/Sdk/$(DOTNET_SDK_VERSION)/dotnet-sdk-$(DOTNET_SDK_VERSION)-osx-arm64.pkg"
          },
          "packageSources": [
            "$(PACKAGE_SOURCE_NUGET_ORG)"
          ],
          "workloads": [
            {
              "workloadId": "android",
              "workloadManifestId": "microsoft.net.sdk.android",
              "packageId": "$(ANDROID_WORKLOAD_MANIFEST_PACKAGE_ID)",
              "version": "$(ANDROID_WORKLOAD_MANIFEST_PACKAGE_VERSION)",
              "sdkBand": "$(ANDROID_WORKLOAD_SDK_BAND)"
            },
            {
              "workloadId": "ios",
              "workloadManifestId": "microsoft.net.sdk.ios",
              "packageId": "$(IOS_WORKLOAD_MANIFEST_PACKAGE_ID)",
              "version": "$(IOS_WORKLOAD_MANIFEST_PACKAGE_VERSION)",
              "sdkBand": "$(IOS_WORKLOAD_SDK_BAND)"
            },
            {
              "workloadId": "maccatalyst",
              "workloadManifestId": "microsoft.net.sdk.maccatalyst",
              "packageId": "$(MACCATALYST_WORKLOAD_MANIFEST_PACKAGE_ID)",
              "version": "$(MACCATALYST_WORKLOAD_MANIFEST_PACKAGE_VERSION)",
              "sdkBand": "$(MACCATALYST_WORKLOAD_SDK_BAND)"
            },
            {
              "workloadId": "tvos",
              "workloadManifestId": "microsoft.net.sdk.tvos",
              "packageId": "$(TVOS_WORKLOAD_MANIFEST_PACKAGE_ID)",
              "version": "$(TVOS_WORKLOAD_MANIFEST_PACKAGE_VERSION)",
              "sdkBand": "$(TVOS_WORKLOAD_SDK_BAND)"
            },
            {
              "workloadId": "macos",
              "workloadManifestId": "microsoft.net.sdk.macos",
              "packageId": "$(MACOS_WORKLOAD_MANIFEST_PACKAGE_ID)",
              "version": "$(MACOS_WORKLOAD_MANIFEST_PACKAGE_VERSION)",
              "sdkBand": "$(MACOS_WORKLOAD_SDK_BAND)"
            },
            {
              "workloadId": "maui",
              "workloadManifestId": "microsoft.net.sdk.maui",
              "packageId": "$(MAUI_WORKLOAD_MANIFEST_PACKAGE_ID)",
              "version": "$(MAUI_WORKLOAD_MANIFEST_PACKAGE_VERSION)",
              "sdkBand": "$(MAUI_WORKLOAD_SDK_BAND)"
            }
          ],
          "packs": []
        }
      ]
    }
  }
}
